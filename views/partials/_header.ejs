<header>
  <nav class="navbar navbar-expand-md navbar-dark nav-light">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      
    <% if(username){ %>
      
      <div class="navbar-nav" style="margin-right: auto !important;">
        <a class="btn-pop-light" href="/urls">My URLs</a>
        <a class="btn-pop-bold" href="/urls/new">Create New URL</a>
       </div>

        <div class="'navbar-nav">
          <form method="POST" action="/logout" for="username" class="d-flex nav-item" role="search">
            <span class="nav-link">Welcome back, <%= username.email %>!</span>
            <button class="btn-exit" type="submit">Logout</button>
          </form>
          </div>

       <% } else{ %>  
        
        <div class="navbar-nav" style="position: sticky; left: 82vw;">
            <a href="/login" class="btn-exit">Login</a>
            <a href="/register" class="btn-exit">Register</a>
         </div>
        <% } %>
      </div>
    </div>
  </nav>
</header>



